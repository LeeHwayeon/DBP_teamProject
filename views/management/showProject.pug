extends ../layout

//- 검색된 프로젝트 출력 페이지
block content
  .container
    .row.align-items-center.mt-4
      .col
        h3.mb-3.ml-3 선택된 프로젝트
        input.form-control(type="text" name="project" value=`${selected}` readonly)

    .row.align-items-center.mt-4
      .col
        table.table.text-center
          thead
            tr
              th(scope="col") 프로젝트 시작일
              th(scope="col") 프로젝트 종료일
              th(scope="col") 발주처
          tbody
            each value in result
              tr
                td=moment(value[0]).format('YYYY-MM-DD')
                td=moment(value[1]).format('YYYY-MM-DD')
                td=value[2]
